def completeDivision(numerador,denominador):
    if denominador==0:
        print("error")
    else:
        print("RESULTADO DIVISION")
        print("Cociente",numerador//denominador)
        print("Residuo",numerador%denominador)
completeDivision(21,7)
                                                                                                                                       

def suma(numero1,numero2):
    print ("Resultado de la suma",numero1+numero2)
suma(2,4)

def resta(numero1,numero2):
    print ("Resultado de la resta",numero1-numero2)
resta(25,20)

def multiplicacion(numero1,numero2):
    print ("Resultado de la multiplicacion",numero1*numero2)
resta(5,25)

def potenciar(base,potencia):
    print ("Resultado de potenciar",base**potencia)
potenciar(5,2)
